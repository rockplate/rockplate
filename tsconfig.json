  "compilerOptions": {
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es2015",
    "strict": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
